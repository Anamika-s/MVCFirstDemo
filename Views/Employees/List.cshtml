
@{
    ViewBag.Title = "List";
}

<h2>List</h2>
<table>
    <tr>
        <th> ID </th>
        <th> Name </th>
        <th> Department </th>
        <th> Salary </th>
    </tr>
    @{ foreach (var employee in ViewBag.list)
        {<tr>
    <td>  @employee.Id </td>

    <td>  @employee.Name </td>


    <td>  @employee.Department </td>

    <td>  @employee.Salary </td>
</tr>
        }
    }
</table>

