@model IEnumerable<MVCDemo.Models.Employee>
@{
    ViewBag.Title = "PassingListToView";
}

<h2>PassingListToView</h2>
<h2>List</h2>
<table>
    <tr>
        <th> ID </th>
        <th> Name </th>
        <th> Department </th>
        <th> Salary </th>
    </tr>
    @{ foreach (var employee in Model)
        {
            <tr>
                <td>  @employee.Id </td>

                <td>  @employee.Name </td>


                <td>  @employee.Department </td>

                <td>  @employee.Salary </td>
            </tr>
        }
    }
</table>



